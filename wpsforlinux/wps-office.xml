<?xml version='1.0' encoding='utf-8'?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">

    <mime-type type="application/wps-office.wps">
        <comment>WPS Writer Document</comment>
        <comment xml:lang="zh_CN">WPS文字 文档</comment>
        <sub-class-of type="application/vnd.ms-works"/>
        <generic-icon name="wps-office-wps"/>
        <glob pattern="*.wps" />
        <glob pattern="*.wpsx" />
    </mime-type>
    <mime-type type="application/wps-office.wpt">
        <comment>WPS Writer Template</comment>
        <comment xml:lang="zh_CN">WPS文字 模板</comment>
        <generic-icon name="wps-office-wpt"/>
        <glob pattern="*.wpt" />
        <glob pattern="*.wptx" />
    </mime-type>
    <mime-type type="application/wps-office.wpss">
        <comment>WPS Writer Security</comment>
        <comment xml:lang="zh_CN">WPS文字 安全</comment>
        <generic-icon name="wps-office-wpss"/>
        <glob pattern="*.wpss" />
    </mime-type>
    <mime-type type="application/wps-office.wpso">
        <comment>WPS Writer Outgoing Document</comment>
        <comment xml:lang="zh_CN">WPS文字 外发文档</comment>
        <generic-icon name="wps-office-wpso"/>
        <glob pattern="*.wpso" />
    </mime-type>

    <mime-type type="application/wps-office.dps">
        <comment>WPS Presentation File</comment>
        <comment xml:lang="zh_CN">WPS演示 演示文稿</comment>
