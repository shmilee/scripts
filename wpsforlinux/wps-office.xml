        <glob pattern="*.wpss" weight="60"/>
    </mime-type>
    <mime-type type="application/wps-office.wpso">
        <comment>WPS Writer Outgoing Document</comment>
        <comment xml:lang="zh_CN">WPS文字 外发文档</comment>
        <generic-icon name="wps-office-wpso"/>
        <glob pattern="*.wpso" weight="60"/>
    </mime-type>

