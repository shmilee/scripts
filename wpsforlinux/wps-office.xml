        <comment xml:lang="zh_CN">WPS演示 模板</comment>
        <generic-icon name="wps-office-dpt"/>
        <glob pattern="*.dpt" />
        <glob pattern="*.dptx" />
    </mime-type>
    <mime-type type="application/wps-office.dpss">
        <comment>WPS Presentation Security</comment>
        <comment xml:lang="zh_CN">WPS演示 安全</comment>
        <generic-icon name="wps-office-dpss"/>
        <glob pattern="*.dpss" />
    </mime-type>
    <mime-type type="application/wps-office.dpso">
        <comment>WPS Presentation Outgoing Documen</comment>
        <comment xml:lang="zh_CN">WPS演示 外发文档</comment>
        <generic-icon name="wps-office-dpso"/>
        <glob pattern="*.dpso" />
