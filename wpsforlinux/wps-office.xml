        <generic-icon name="wps-office-et"/>
        <glob pattern="*.et" />
        <glob pattern="*.etx" />
    </mime-type>
    <mime-type type="application/wps-office.ett">
        <comment>WPS Spreadsheets Template</comment>
        <comment xml:lang="zh_CN">WPS表格 模板</comment>
        <generic-icon name="wps-office-ett"/>
        <glob pattern="*.ett" />
        <glob pattern="*.ettx" />
    </mime-type>
    <mime-type type="application/wps-office.ets">
        <comment>WPS Spreadsheets Security</comment>
        <comment xml:lang="zh_CN">WPS表格 安全</comment>
        <generic-icon name="wps-office-ets"/>
        <glob pattern="*.ets" />
